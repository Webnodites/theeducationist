    const queryString = window.location.search;
    const urlParams = new URLSearchParams(queryString);
    const user = urlParams.get('email') || "";
    Autopilot.run("associate", user);
  })();
</script>

<script src="https://embed.small.chat/T024T59MDG010LH75YUE.js" async ></script>

<script>
  var d = new Date();
  var year = d.getFullYear();
  $('#year').html(year)
</script>

<script>
  $('.tab-link').hover(
    function() {
      var $parent = $(this).parent();
      var tab = $(this).data('tab');
      $parent.children('.tab-link.current').removeClass('current');
      $(this).addClass('current');
      $(`.w-tab-link[data-w-tab=${tab}`).click();
    },
    
    function() {
      $(this).removeClass('current');
    }
  );
</script>

