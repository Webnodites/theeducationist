var Webflow = Webflow || [];
Webflow.push(function () {
  $('.legal__dropdown-link').click(function() {
    const tab = $(this).data('tab');
    const value = $(this).text();
    $(`.w-tab-link[data-w-tab=${tab}]`).click();
    $('.legal__dropdown-selected').text(value);
    $('.legal__dropdown-toggle').click();
    
    sessionStorage.setItem('region', tab);
    sessionStorage.setItem('regionName', value);
  });
  
  let region = sessionStorage.getItem('region');
  if (region) {
  	let name = $(`.legal__dropdown-link[data-tab=${region}]`).text();
    $(`.w-tab-link[data-w-tab=${region}]`).click();
    $('.legal__dropdown-selected').text(name);
  }
});
