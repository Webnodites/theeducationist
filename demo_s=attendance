<script>
  $('.tab-link').hover(
    function() {
      var $parent = $(this).parent();
      var tab = $(this).data('tab');
      $parent.children('.tab-link.current').removeClass('current');
      $(this).addClass('current');
      $(`.w-tab-link[data-w-tab=${tab}`).click();
    },
    
    function() {
      $(this).removeClass('current');
    }
  );
</script>

